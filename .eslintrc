{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jsx-a11y",
      "react"
    ],
    "rules": {
        "comma-dangle": [2, "never"],
        "semi": [2, "never"],
        "react/prop-types": "off",
        "react/jsx-filename-extension": "off"
    },
    "globals": {
        "window": false,
        "document": false
    }
}
